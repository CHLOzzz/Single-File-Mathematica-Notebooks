Cell[TextData[{
 "Because ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "n"], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2d796efa-87df-45a7-b29d-582819247175"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"n", "+", "1"}]], ">", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e018817d-beae-45de-87a1-39a204a0b767"],
 " is outside of our domain. So if we numerically approximate up to ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", "(", 
     SubscriptBox["x", 
      RowBox[{"n", "+", "1"}]], ")"}], "=", 
    RowBox[{"u", "(", 
     RowBox[{"b", "+", "h"}], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "99c36b7b-2be6-4cde-adc5-96cfb80f1834"],
 ", we are no longer obtaining an approximation of ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cdfbc240-7da8-48f0-a1a7-77f180213c8c"],
 " in our domain ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"a", ",", "b"}], "]"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "212727be-8d97-4515-a10b-610a2aafbc7b"],
 "."
}], "Text",
