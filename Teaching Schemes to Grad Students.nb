Cell[TextData[{
 "Because ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "n"], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1fa27599-3a0e-4a56-a000-bf3659005ccf"],
