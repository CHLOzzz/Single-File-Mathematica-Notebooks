 Cell[BoxData[
  FormBox["h", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6486a595-873c-423f-aae9-0128df75662d"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "(", 
    SubscriptBox["x", "0"], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f5200f18-ace7-4ec0-8fb3-89fa58156b31"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "(", 
    SubscriptBox["x", "0"], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ad4c4df4-6d50-468c-ba6e-b61a0bb5171b"],
