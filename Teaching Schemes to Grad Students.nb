Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Input", " ", "your", " ", "custom", " ", "variables"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", " ", "b"}], "}"}], " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{"2", "*", "\[Pi]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"u0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"scheme", "[", 
      RowBox[{"inF_", ",", " ", 
       RowBox[{"{", 
        RowBox[{"inA_", ",", " ", "inB_"}], "}"}], ",", " ", "inU0_"}], "]"}],
      " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "trueSol", ",", " ", "u", ",", " ", "x", ",", " ", "fastF", ",", " ", 
         "h", ",", " ", "n", ",", " ", "j", ",", " ", "i", ",", " ", 
         "approxSol", ",", " ", "err", ",", " ", "\[Alpha]", ",", " ", 
         "plotLabel"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Error", " ", "check", " ", "domain"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"inA", ">=", "inB"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a<b\",ExpressionUUID->\"46bb2b5b-acc9-425a-a365-5076e2b748e9\"]\)\
...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Or", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Head", "[", "inA", "]"}], "==", "Complex"}], ",", " ", 
            RowBox[{
             RowBox[{"Head", "[", "inB", "]"}], "==", "Complex"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a\",ExpressionUUID->\"f5168564-d1ec-42bd-93ff-7ebdfbe2a6b9\"]\) \
and \!\(\*Cell[\"b\",ExpressionUUID->\"308a092e-c3d2-4979-b4f6-7f884efbeacd\"]\
\) to be real...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Obtain", " ", "true", " ", "solution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"trueSol", " ", "=", " ", 
         RowBox[{"NDSolveValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"u", "'"}], "[", "x", "]"}], "==", 
              RowBox[{"inF", "[", "x", "]"}]}], ",", " ", 
             RowBox[{
              RowBox[{"u", "[", "inA", "]"}], "==", "inU0"}]}], "}"}], ",", 
           " ", "u", ",", " ", 
           RowBox[{"{", 
            RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Compile", " ", "f", " ", "so", " ", "code", " ", "runs", " ", 
          "faster"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"fastF", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "_Real"}], "}"}], "}"}], ",", " ", 
           RowBox[{"inF", "[", "x", "]"}], ",", " ", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", Cell[
          "h",ExpressionUUID->"42f34c46-b726-4954-a674-1730bf0a1a09"], " ", 
          "delayed", " ", "for", " ", "manipulating", " ", "across", " ", 
          Cell["n",ExpressionUUID->"b77b883b-4e44-424b-b426-8f0892e66b9a"]}], 
         " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"N", "[", 
          FractionBox[
           RowBox[{"inB", "-", "inA"}], "n"], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "approximate", " ", "solution", " ", "delayed", " ", 
          "for", " ", "manipulating", " ", "across", " ", Cell[
          "n",ExpressionUUID->"2b9c8ee4-edd2-4513-b05d-76fe66173d60"]}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"approxSol", "[", 
          RowBox[{"n_", ",", " ", "\[Alpha]_"}], "]"}], " ", ":=", " ", 
         RowBox[{"Thread", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"NestList", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"h", "[", "n", "]"}], "+", "#"}], "&"}], ",", " ", 
              "inA", ",", " ", "n"}], "]"}], ",", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"j", " ", "=", " ", 
               RowBox[{"1", "-", "\[Alpha]"}]}], ";", 
              RowBox[{"NestList", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"h", "[", "n", "]"}], "*", 
                   RowBox[{"fastF", "[", 
                    RowBox[{"inA", "+", 
                    RowBox[{
                    RowBox[{"h", "[", "n", "]"}], "*", 
                    RowBox[{"j", "++"}]}]}], "]"}]}], "+", "#"}], "&"}], ",", 
                " ", "inU0", ",", " ", "n"}], "]"}]}], ")"}]}], "}"}], 
          "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "error", " ", "delayed", " ", "for", " ", 
          "manipulating", " ", "across", " ", "n"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"err", "[", 
          RowBox[{"n_", ",", " ", "\[Alpha]_"}], "]"}], " ", ":=", " ", 
         RowBox[{"Norm", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"approxSol", "[", 
              RowBox[{"n", ",", " ", "\[Alpha]"}], "]"}], "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", " ", "2"}], "]"}], "]"}], "-", 
            RowBox[{"trueSol", "[", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{"inA", "+", 
                RowBox[{"i", "*", 
                 RowBox[{"h", "[", "n", "]"}]}]}], ",", " ", 
               RowBox[{"{", 
                RowBox[{"i", ",", " ", "0", ",", " ", "n"}], "}"}]}], "]"}], 
             "]"}]}], ",", " ", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Condense", " ", "\[Alpha]", " ", "indicator", " ", "to", " ", 
          "properly", " ", "label", " ", "plot"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"plotLabel", "[", "\[Alpha]_", "]"}], " ", ":=", " ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"\[Alpha]", "==", "1"}], ",", " ", "\"\<Forward Euler\>\"",
            ",", " ", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"\[Alpha]", "==", "0"}], ",", " ", 
             "\"\<Backward Euler\>\"", ",", " ", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"\[Alpha]", "==", 
                FractionBox["1", "2"]}], ",", " ", 
               "\"\<Central Difference\>\"", ",", " ", 
               "\"\<Skewed Difference\>\""}], "]"}]}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Code", " ", "to", " ", "display", " ", "the", " ", "approximation", 
          " ", "visually"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"Manipulate", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Show", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Plot", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"trueSol", "[", "x", "]"}], ",", " ", 
              RowBox[{"{", 
               RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"PlotLabel", "->", 
               RowBox[{
                RowBox[{"plotLabel", "[", "\[Alpha]", "]"}], "<>", 
                "\"\< Scheme\\nEuclidean Error: \>\"", "<>", 
                RowBox[{"ToString", "[", 
                 RowBox[{"err", "[", 
                  RowBox[{"n", ",", " ", "\[Alpha]"}], "]"}], 
                 "\[IndentingNewLine]", "]"}]}]}]}], "]"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"ListPlot", "[", 
             RowBox[{
              RowBox[{"approxSol", "[", 
               RowBox[{"n", ",", " ", "\[Alpha]"}], "]"}], ",", " ", 
              RowBox[{"PlotStyle", "->", "Red"}]}], "]"}]}], 
           "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"n", ",", " ", "20", ",", " ", "\"\<Nodes\>\""}], "}"}], 
            ",", " ", "1", ",", " ", "2000", ",", " ", "1"}], "}"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"\[Alpha]", ",", " ", "0.75", ",", " ", "\"\<Skew\>\""}],
              "}"}], ",", " ", "0", ",", " ", "1"}], "}"}]}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Call", " ", "code", " ", "to", " ", "run"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"scheme", "[", 
