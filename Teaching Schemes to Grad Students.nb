Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Input", " ", "your", " ", "custom", " ", "variables"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", " ", "b"}], "}"}], " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{"2", "*", "\[Pi]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"u0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"scheme", "[", 
      RowBox[{"inF_", ",", " ", 
       RowBox[{"{", 
        RowBox[{"inA_", ",", " ", "inB_"}], "}"}], ",", " ", "inU0_"}], "]"}],
      " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "trueSol", ",", "fastF", ",", "u", ",", "x", ",", "\[Phi]", ",", 
         "D\[Phi]", ",", "\[Epsilon]", ",", "xi", ",", "xj", ",", "matA", ",",
          "n", ",", "choice", ",", "h", ",", "i", ",", "j", ",", "vecF", ",", 
         "sol\[Lambda]", ",", "approx", ",", "mChoice", ",", "m\[Epsilon]", 
         ",", "mn", ",", "thisH", ",", "this\[Lambda]", ",", "l2err"}], "}"}],
        ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Error", " ", "check", " ", "domain"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"inA", ">=", "inB"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a<b\",ExpressionUUID->\"3f15f97a-2b5d-4b29-bc1e-58e0ee30c534\"]\)\
...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Or", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Head", "[", "inA", "]"}], "==", "Complex"}], ",", " ", 
            RowBox[{
             RowBox[{"Head", "[", "inB", "]"}], "==", "Complex"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a\",ExpressionUUID->\"cb0d5510-97fe-44b1-8022-bea3613e5acc\"]\) \
and \!\(\*Cell[\"b\",ExpressionUUID->\"fa9f5475-6817-4ef7-adfc-0adf9c4cd770\"]\
\) to be real...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Obtain", " ", "true", " ", "solution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"trueSol", " ", "=", " ", 
         RowBox[{"NDSolveValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"u", "'"}], "[", "x", "]"}], "==", 
              RowBox[{"inF", "[", "x", "]"}]}], ",", " ", 
             RowBox[{
              RowBox[{"u", "[", "inA", "]"}], "==", "inU0"}]}], "}"}], ",", 
           " ", "u", ",", " ", 
           RowBox[{"{", 
            RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Compile", " ", "all", " ", "functions", " ", "used", " ", 
          "repeatedly", " ", "for", " ", "faster", " ", "code"}], " ", "*)"}],
         "\[IndentingNewLine]", 
        RowBox[{"fastF", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "_Real"}], "}"}], "}"}], ",", " ", 
           RowBox[{"inF", "[", "x", "]"}], ",", " ", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"\[Phi]", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"choice", ",", " ", "_Integer"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"\[Epsilon]", ",", " ", "_Real"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"xi", ",", " ", "_Real"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"xj", ",", " ", "_Real"}], "}"}]}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Switch", "[", 
            RowBox[{"choice", ",", "\[IndentingNewLine]", "1", ",", " ", 
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{"-", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{"\[Epsilon]", "*", 
                  RowBox[{"Abs", "[", 
                   RowBox[{"xj", "-", "xi"}], "]"}]}], ")"}], "2"]}]], ",", 
             "\[IndentingNewLine]", "2", ",", " ", 
             SqrtBox[
              RowBox[{"1", "+", 
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{"\[Epsilon]", "*", 
                  RowBox[{"Abs", "[", 
                   RowBox[{"xj", "-", "xi"}], "]"}]}], ")"}], "2"]}]], ",", 
             "\[IndentingNewLine]", "3", ",", " ", 
             FractionBox["1", 
              SqrtBox[
               RowBox[{"1", "+", 
                SuperscriptBox[
                 RowBox[{"(", 
                  RowBox[{"\[Epsilon]", "*", 
                   RowBox[{"Abs", "[", 
                    RowBox[{"xj", "-", "xi"}], "]"}]}], ")"}], "2"]}]]]}], 
            "]"}], ",", "\[IndentingNewLine]", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"D\[Phi]", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"choice", ",", " ", "_Integer"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"\[Epsilon]", ",", " ", "_Real"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"xi", ",", " ", "_Real"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{"xj", ",", " ", "_Real"}], "}"}]}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"xi", "==", "xj"}], ",", " ", "0", ",", " ", 
             RowBox[{"Switch", "[", 
              RowBox[{"choice", ",", "\[IndentingNewLine]", "1", ",", " ", 
               RowBox[{
                RowBox[{"-", "2"}], " ", 
                SuperscriptBox["\[ExponentialE]", 
                 RowBox[{
                  RowBox[{"-", 
                   SuperscriptBox["\[Epsilon]", "2"]}], " ", 
                  SuperscriptBox[
                   RowBox[{"Abs", "[", 
                    RowBox[{
                    RowBox[{"-", "xi"}], "+", "xj"}], "]"}], "2"]}]], " ", 
                SuperscriptBox["\[Epsilon]", "2"], " ", 
                RowBox[{"Abs", "[", 
                 RowBox[{
                  RowBox[{"-", "xi"}], "+", "xj"}], "]"}], " ", 
                RowBox[{"Sign", "[", 
                 RowBox[{
                  RowBox[{"-", "xi"}], "+", "xj"}], "]"}]}], ",", 
               "\[IndentingNewLine]", "2", ",", " ", 
               FractionBox[
                RowBox[{
                 SuperscriptBox["\[Epsilon]", "2"], " ", 
                 RowBox[{"Abs", "[", 
                  RowBox[{
                   RowBox[{"-", "xi"}], "+", "xj"}], "]"}], " ", 
                 RowBox[{"Sign", "[", 
                  RowBox[{
                   RowBox[{"-", "xi"}], "+", "xj"}], "]"}]}], 
                SqrtBox[
                 RowBox[{"1", "+", 
                  RowBox[{
                   SuperscriptBox["\[Epsilon]", "2"], " ", 
                   SuperscriptBox[
                    RowBox[{"Abs", "[", 
                    RowBox[{
                    RowBox[{"-", "xi"}], "+", "xj"}], "]"}], "2"]}]}]]], ",", 
               "\[IndentingNewLine]", "3", ",", " ", 
               RowBox[{"-", 
                FractionBox[
                 RowBox[{
                  SuperscriptBox["\[Epsilon]", "2"], " ", 
                  RowBox[{"Abs", "[", 
                   RowBox[{
                    RowBox[{"-", "xi"}], "+", "xj"}], "]"}], " ", 
                  RowBox[{"Sign", "[", 
                   RowBox[{
                    RowBox[{"-", "xi"}], "+", "xj"}], "]"}]}], 
                 SuperscriptBox[
                  RowBox[{"(", 
                   RowBox[{"1", "+", 
                    RowBox[{
                    SuperscriptBox["\[Epsilon]", "2"], " ", 
                    SuperscriptBox[
                    RowBox[{"Abs", "[", 
                    RowBox[{
                    RowBox[{"-", "xi"}], "+", "xj"}], "]"}], "2"]}]}], ")"}], 
                  RowBox[{"3", "/", "2"}]]]}]}], "]"}]}], "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", Cell[
          "h",ExpressionUUID->"c90e6cc5-ae6e-4b9b-8843-b70cff3ba7cf"], " ", 
          "delayed", " ", "for", " ", "manipulating", " ", "across", " ", 
          Cell["n",ExpressionUUID->"02d91566-802c-43e3-bc3d-c6297a57d1f8"]}], 
         " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"N", "[", 
          FractionBox[
           RowBox[{"inB", "-", "inA"}], "n"], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{
          "Define", " ", Cell[
           "A",ExpressionUUID->"b8ff6327-ee60-4835-8ed1-fc27e5657410"], " ", 
           "delayed", " ", "for", " ", "manipulating", " ", "across", " ", 
           Cell["n",ExpressionUUID->"e4d66eac-52a3-44e4-85bc-b08a134683c9"]}],
           ",", " ", Cell[
          "\[Epsilon]",ExpressionUUID->
           "8c7e47ea-b1c2-4068-ba04-f8857b2f9ed9"], ",", " ", 
          RowBox[{
          Cell["choice",ExpressionUUID->
            "78c9d232-625e-4718-9bbd-6b35b601ddaa"], " ", "of", " ", Cell[
           "\[Phi]",ExpressionUUID->
            "92b66aa7-c763-4b96-a887-b8206030806b"]}]}], " ", "*)"}], 
