Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Clear", " ", "all", " ", "variables", " ", "to", " ", "ensure", " ", 
    "code", " ", "below", " ", "works"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Input", " ", "your", " ", "custom", " ", "variables"}], " ", 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"f", "[", "x_", "]"}], " ", "=", " ", 
     RowBox[{"Cos", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", " ", "b"}], "}"}], " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"0", ",", " ", 
       RowBox[{"2", "*", "\[Pi]"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"u0", " ", "=", " ", "0"}], ";"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Code", " ", "that", " ", "constructs", " ", "the", " ", "visual", " ", 
     "of", " ", "the", " ", "scheme", " ", "approximating", " ", "the", " ", 
     "true", " ", "solution"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"scheme", "[", 
      RowBox[{"inF_", ",", " ", 
       RowBox[{"{", 
        RowBox[{"inA_", ",", " ", "inB_"}], "}"}], ",", " ", "inU0_"}], "]"}],
      " ", ":=", " ", 
     RowBox[{"DynamicModule", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "trueSol", ",", " ", "u", ",", " ", "x", ",", " ", "fastF", ",", " ", 
         "h", ",", " ", "n", ",", " ", "j", ",", " ", "i", ",", " ", 
         "approxSol", ",", " ", "err"}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{"Error", " ", "check", " ", "domain"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"inA", ">=", "inB"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a<b\",ExpressionUUID->\"1b96a737-b57c-4300-b943-6dba8c90bab3\"]\)\
...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"Or", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Head", "[", "inA", "]"}], "==", "Complex"}], ",", " ", 
            RowBox[{
             RowBox[{"Head", "[", "inB", "]"}], "==", "Complex"}]}], "]"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", 
            RowBox[{"Style", "[", 
             RowBox[{
             "\"\<ERROR: Invalid domain! Requires \
\!\(\*Cell[\"a\",ExpressionUUID->\"6d70403c-0d63-4716-a625-a2a92a870d1e\"]\) \
and \!\(\*Cell[\"b\",ExpressionUUID->\"06ad3de2-f575-465f-8cf9-82fd85f25a67\"]\
\) to be real...\>\"", ",", " ", "Red"}], "]"}], "]"}], ";", 
           RowBox[{"Return", "[", 
            RowBox[{"Null", ",", " ", "Module"}], "]"}]}], ",", 
          "\[IndentingNewLine]", "Null"}], "]"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Obtain", " ", "true", " ", "solution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"trueSol", " ", "=", " ", 
         RowBox[{"NDSolveValue", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"u", "'"}], "[", "x", "]"}], "==", 
              RowBox[{"inF", "[", "x", "]"}]}], ",", " ", 
             RowBox[{
              RowBox[{"u", "[", "inA", "]"}], "==", "inU0"}]}], "}"}], ",", 
           " ", "u", ",", " ", 
           RowBox[{"{", 
            RowBox[{"x", ",", " ", "inA", ",", " ", "inB"}], "}"}]}], "]"}]}],
         ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Compile", " ", "f", " ", "so", " ", "code", " ", "runs", " ", 
          "faster"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"fastF", " ", "=", " ", 
         RowBox[{"Compile", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"x", ",", " ", "_Real"}], "}"}], "}"}], ",", " ", 
           RowBox[{"inF", "[", "x", "]"}], ",", " ", 
           RowBox[{"CompilationTarget", "->", "\"\<C\>\""}], ",", " ", 
           RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", Cell[
          "h",ExpressionUUID->"0d8cbc77-9efe-4c09-8261-3e236a04958d"], " ", 
          "delayed", " ", "for", " ", "manipulating", " ", "across", " ", 
          Cell["n",ExpressionUUID->"47a3cc62-c571-44d2-88b7-37b739b8a0bb"]}], 
         " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"h", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"N", "[", 
          FractionBox[
           RowBox[{"inB", "-", "inA"}], "n"], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "approximate", " ", "solution", " ", "delayed", " ", 
          "for", " ", "manipulating", " ", "across", " ", Cell[
          "n",ExpressionUUID->"0bdc6524-d64f-4a4d-b0ef-3a0f4e835501"]}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"approxSol", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Thread", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"NestList", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"h", "[", "n", "]"}], "+", "#"}], "&"}], ",", " ", 
              "inA", ",", " ", "n"}], "]"}], ",", " ", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"j", " ", "=", " ", 
               FractionBox["1", "2"]}], ";", 
              RowBox[{"NestList", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"h", "[", "n", "]"}], "*", 
                   RowBox[{"fastF", "[", 
                    RowBox[{"inA", "+", 
                    RowBox[{
                    RowBox[{"h", "[", "n", "]"}], "*", 
                    RowBox[{"j", "++"}]}]}], "]"}]}], "+", "#"}], "&"}], ",", 
                " ", "inU0", ",", " ", "n"}], "]"}]}], ")"}]}], "}"}], 
          "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Define", " ", "error", " ", "delayed", " ", "for", " ", 
          "manipulating", " ", "across", " ", "n"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"err", "[", "n_", "]"}], " ", ":=", " ", 
         RowBox[{"Norm", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"approxSol", "[", "n", "]"}], "[", 
             RowBox[{"[", 
              RowBox[{"All", ",", " ", "2"}], "]"}], "]"}], "-", 
            RowBox[{"trueSol", "[", 
